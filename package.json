{
  "name": "@shanknet/http-request",
  "version": "0.0.9",
  "description": "Simple wrapper module for making http requests on the client.",
  "license": "MIT",
  "author": "clsop <shanknet85@gmail.com>",
  "main": "src/index.js",
  "typings": "src/typings/index.d.ts",
  "nyc": {
    "include": [
      "test/tmp/src/**/*.js"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".js"
    ],
    "require": [
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "text"
    ],
    "all": false,
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "build": "tsc --build .",
    "pretest": "tsc --outDir test/tmp",
    "test": "nyc mocha"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.1.2",
    "@types/sinon": "^7.0.3",
    "mocha": "^5.0.4",
    "mocha-typescript": "^1.1.12",
    "nyc": "^13.1.0",
    "should": "^13.2.1",
    "sinon": "^7.2.2",
    "source-map-support": "^0.5.9",
    "typescript": "^3.2.2"
  }
}
